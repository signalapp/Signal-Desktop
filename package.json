    "@types/filesize": "3.6.0",
    "@types/firstline": "^2.0.2",
    "@types/fs-extra": "5.0.5",
    "@types/libsodium-wrappers-sumo": "^0.7.5",
    "@types/linkify-it": "^3.0.2",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "5.0.0",
    "@types/node-fetch": "^2.5.7",
