    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-h5-audio-player": "^3.2.0",
    "react-intersection-observer": "^9.7.0",
    "react-mentions": "^4.4.9",
    "react-qr-svg": "^2.2.1",
